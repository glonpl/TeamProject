<form method="post" action="{{ path('symptoms_delete', {'id': symptom.id}) }}" onsubmit="return confirm('Jesteś pewny?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ symptom.id) }}">
    <button class="btn btn-danger">Usuń</button>
</form>
